{
  "compilerOptions": {
    "module": "ES2015",
    "target": "ES2019",
    "lib": ["ES2019"],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noImplicitAny": true
  },
  "include": [
    "src/**/*"
  ],
}
